<Page x:Class="SQL_Injection_Playground.View.FinishTaskPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SQL_Injection_Playground.View"
      mc:Ignorable="d" 
      d:DesignHeight="434" d:DesignWidth="620"
      Title="Task Finished"
      Background="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}">

    <Grid>
        <TextBlock x:Name="Msg_Log" HorizontalAlignment="Stretch" Margin="0,0,0,5" VerticalAlignment="Bottom" FontSize="25" TextWrapping="Wrap" TextAlignment="Center"/>
        <TextBlock x:Name="Tb_Message" HorizontalAlignment="Stretch" Margin="100,100,100,100" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Stretch" FontSize="20" TextAlignment="Center"/>
    </Grid>
</Page>
